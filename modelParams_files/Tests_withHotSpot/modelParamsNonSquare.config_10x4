[DEFAULT]
label = 

[Path]
home = ./../
library = %(home)slib/
flp = %(home)sflp_files/
ptrace = %(home)sptrace_files/

[Solver]
name = SuperLU
wrapper = SuperLU.py

[Grid]
type = Uniform
granularity = Grid
rows = 10
cols = 4

#For information#
[VirtualNodes]
center_center = 0.5
bottom_center = 1

[NoPackage]
LateralHeatFlow = True
VerticalHeatFlow = True
library_name = NoPackage_sec
library = Solid.py
virtual_node = bottom_center
mode = single
#transient = False

[NoPackage_sec]
properties = htc, thickness (m), thermalresistivity ((m-k)/w), specificheatcapacity (j/m^3k) 

[Si]
library_name = Solid
library = Solid.py
transient = True
virtual_node = bottom_center
mode = single

[Solid]
properties = thermalresistivity ((m-k)/w), specificheatcapacity (j/m^3k)

