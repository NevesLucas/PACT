[DEFAULT]
label = 

[Path]
home = ./../
library = %(home)slib/
flp = %(home)sflp_files/
ptrace = %(home)sptrace_files/

[Solver]
name = SuperLU
wrapper = SuperLU.py

[Grid]
type = Uniform
granularity = Grid
rows = 80
cols = 80

#DO NOT CHANGE BELOW
[VirtualNodes]
center_center = 0.5
bottom_center = 1

# Below is for information only: It's hard coded in the tool
#Start
[NoPackage]
LateralHeatFlow = True
VerticalHeatFlow = True
library_name = NoPackage_sec
library = Solid.py
virtual_node = bottom_center
#transient = False

[NoPackage_sec]
properties = htc, thickness (m), thermalresistivity ((m-k)/w), specificheatcapacity (j/m^3k) 
#Stop

[Si]
library_name = Solid
library = Solid.py
transient = True
virtual_node = bottom_center

[Solid]
properties = thermalresistivity ((m-k)/w), specificheatcapacity (j/m^3k)

[Liquid]
vitual_node = center_center

